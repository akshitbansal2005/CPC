#include <iostream>
using namespace std;

int main(){
    int age = 19;
    
    if(age<=17){
        cout<<"child"<<endl;
    }
    
    else if(age>17 && age<=18){
        cout<<"adult"<<endl;
    }
    
    else if(age>18 && age<=65){
        cout<<"adult"<<endl;
    }
    
    else{
        cout<<"senior citizen"<<endl;
    }
}
